{
  "name": "ngVue",
  "author": "Doray Hong <hongduhui@gmail.com>",
  "version": "0.1.2",
  "description": "Use Vue Components in Angular 1.x",
  "main": "build/ngVue.umd.js",
  "jsnext:main": "build/ngVue.es.js",
  "keywords": [
    "javascript",
    "migration",
    "vue",
    "angular",
    "ng",
    "components",
    "ngvue"
  ],
  "scripts": {
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register --recursive",
    "dev-example": "npm run bundle && BABEL_ENV=dev webpack-dev-server --inline --hot --config ./example/webpack.config.js --content-base ./example",
    "clean": "rm -rf build",
    "minify": "uglifyjs ./build/ngVue.umd.js -c -m -o ./build/ngVue.min.js",
    "bundle": "BABEL_ENV=build rollup -c",
    "lint": "eslint src lib test example",
    "build": "npm run lint && npm run clean && npm run bundle && npm run minify",
    "preversion": "npm run test",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "uglifyjs": "^2.4.10",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngVue/ngVue.git"
  },
  "bugs": {
    "url": "https://github.com/ngVue/ngVue/issues"
  },
  "homepage": "https://github.com/ngVue/ngVue#readme",
  "license": "MIT"
}
